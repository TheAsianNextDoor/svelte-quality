{
  "name": "solid-quality",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "check-updates": "ncu",
    "clean": "rm -rf ./node_modules & rm ./package-lock.json",
    "coverage": "NODE_ENV=test vitest run --coverage",
    "coverage:view": "NODE_ENV=test npm run coverage && open ./reports/index.html",
    "db:gui": "prisma studio",
    "db:pull": "prisma pull",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:types": "prisma generate",
    "db:update": "prisma pull && prisma push && prisma generate",
    "dev": "vite dev",
    "lint": "concurrently -n prettier, eslint \"prettier --plugin-search-dir . --check .\" \"eslint .\"",
    "lint:fix": "concurrently -n prettier, eslint \"prettier . --write\" \"eslint . --fix\"",
    "playwright:test": "playwright test --project=chromium --project=firefox",
    "playwright:debug-test": "playwright test --project=chromium --debug --",
    "preview": "vite preview",
    "test": "NODE_ENV=test vitest run",
    "test:watch": "NODE_ENV=test vitest watch",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "prisma": "^4.6.1"
  },
  "devDependencies": {
    "@playwright/test": "1.25.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vitest/ui": "^0.25.2",
    "autoprefixer": "^10.4.13",
    "carbon-components-svelte": "^0.70.12",
    "carbon-icons-svelte": "^11.4.0",
    "carbon-preprocess-svelte": "^0.9.1",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-vitest-globals": "^1.2.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "msw": "^0.49.0",
    "npm-check-updates": "^16.4.1",
    "postcss": "^8.4.19",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rollup-plugin-visualizer": "^5.8.3",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.0",
    "vite-plugin-checker": "^0.5.1",
    "vite-plugin-environment": "^1.1.3",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.25.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
